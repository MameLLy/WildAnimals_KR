<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="512.0" prefWidth="1030.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.wildanimals.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextField promptText="Введите вид животного" />
   <TextField promptText="Ввод номера лицензии" />
   <ComboBox prefWidth="150.0" promptText="Классы животных" />
   <ComboBox prefWidth="150.0" promptText="Вид корма" />

    <Label fx:id="welcomeText" />
   <Button mnemonicParsing="false" text="Добавить животное" />
   <TableView prefHeight="200.0" prefWidth="200.0">
     <columns>
       <TableColumn prefWidth="150.0" text="Вид животного" />
       <TableColumn minWidth="0.0" prefWidth="0.0" text="C2" />
         <TableColumn prefWidth="122.0" text="Класс животного" />
         <TableColumn prefWidth="227.0" text="Вид корма" />
         <TableColumn prefWidth="165.0" text="Номер лицензии" />
     </columns>
   </TableView>
</VBox>
